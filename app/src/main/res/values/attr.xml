<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyPasswordView">
        <attr name="MyBackgroundColor" format="color|reference"/>
        <attr name="MyPasswordLength" format="integer|reference"/>
        <attr name="MyPasswordLineWith" format="float|reference"/>
        <attr name="MyPasswordLineColor" format="color|reference"/>
        <attr name="MyPasswordColor" format="color|reference"/>
        <attr name="MyPasswordSize" format="integer|reference"/>
    </declare-styleable>

    <declare-styleable name="SwipeMenuView">
        <attr name="swipeEnable" format="boolean"/>
        <attr name="ios" format="boolean"/>
        <attr name="leftSwipe" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MainNavigateTabBar">
        <attr name="navigateTabTextColor" format="color|reference"/>
        <attr name="navigateTabSelectedTextColor" format="color|reference"/>
        <attr name="containerId" format="integer|reference"/>
        <attr name="navigateTabTextSize" format="dimension|reference"/>
    </declare-styleable>

</resources>
